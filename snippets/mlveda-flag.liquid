      var symbol="";
      if(!currencyMap[jQuery(this).val()]){
        currencyMap[jQuery(this).val()] = {};
        currencyMap[jQuery(this).val()].symbol_native = "";
      }else{
      symbol = " - "+currencyMap[jQuery(this).val()].symbol_native;
      }
