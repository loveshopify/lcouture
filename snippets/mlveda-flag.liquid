     });

   
    // Option click
    $acs(document).on('click.nice_select', '.nice-select .option:not(.disabled)', function(event) {
      var option = $acs(this);
      var dropdown = option.closest('.nice-select');
      
      dropdown.find('.selected').removeClass('selected');
      option.addClass('selected');
      
      var text = option.data('display') || option.text();
      text=text.trim();
       dropdown.find('.current').text(text);
       {% if settings.mlvedaACS_currencysymbol == true %}
          {% if settings.mlvedaACS_currencysymbolposition == "left" %}
             text= text.substring(text.length - 3);
          {% endif %}
          {% if settings.mlvedaACS_currencysymbolposition == "right" %}
             text=text.substring(0,3);
          {% endif %}  
      {% endif %}
     
      
    
      dropdown.find(".current-flags").removeClass (function (index, className) {
        return (className.match (/{{ settings.mlvedaACS_flagStyle }}-\S+/g) || []).join(' ');
});
                
      {% if settings.mlvedaACS_currencysymbol == true %}
          {% if settings.mlvedaACS_currencysymbolposition == "left" %}
           dropdown.find(".current-flags").addClass("{{ settings.mlvedaACS_flagStyle }}-"+text.toLowerCase());
          {% endif %}
          {% if settings.mlvedaACS_currencysymbolposition == "right" %}
          dropdown.find(".current-flags").addClass("{{ settings.mlvedaACS_flagStyle }}-"+text.toLowerCase());
          {% endif %}                                
      {% else %}
                dropdown.find(".current-flags").addClass("{{ settings.mlvedaACS_flagStyle }}-"+text.toLowerCase());                                   
      {% endif %}
                                                   
                
    //  dropdown.find('.current-flags').css("background","url("+flaglink+text.toLowerCase()+".png) left center no-repeat");
//       dropdown.find('.current-flags').css("background-size","20px 15px");
      
      dropdown.prev('select').val(option.data('value')).trigger('change');
    });

    // Keyboard events
    $acs(document).on('keydown.nice_select', '.nice-select', function(event) {    
      var dropdown = $acs(this);
      var focused_option = $acs(dropdown.find('.focus') || dropdown.find('.list .option.selected'));
      
      // Space or Enter
      if (event.keyCode == 32 || event.keyCode == 13) {
        if (dropdown.hasClass('open')) {
          focused_option.trigger('click');
        } else {
          dropdown.trigger('click');
        }
        return false;
      // Down
      } else if (event.keyCode == 40) {
        if (!dropdown.hasClass('open')) {
          dropdown.trigger('click');
        } else {
          var next = focused_option.nextAll('.option:not(.disabled)').first();
          if (next.length > 0) {
            dropdown.find('.focus').removeClass('focus');
            next.addClass('focus');
          }
        }
        return false;
      // Up
      } else if (event.keyCode == 38) {
        if (!dropdown.hasClass('open')) {
          dropdown.trigger('click');
        } else {
          var prev = focused_option.prevAll('.option:not(.disabled)').first();
          if (prev.length > 0) {
            dropdown.find('.focus').removeClass('focus');
            prev.addClass('focus');
          }
        }
        return false;
      // Esc
      } else if (event.keyCode == 27) {
        if (dropdown.hasClass('open')) {
          dropdown.trigger('click');
        }
      // Tab
      } else if (event.keyCode == 9) {
        if (dropdown.hasClass('open')) {
          return false;
        }
      }
    });

    // Detect CSS pointer-events support, for IE <= 10. From Modernizr.
    var style = document.createElement('a').style;
    style.cssText = 'pointer-events:auto';
    if (style.pointerEvents !== 'auto') {
      $acs('html').addClass('no-csspointerevents');
    }

  };

